{
    "name": "YBR Extension",
    "description": "YBR Product Listing Extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"],
        "persistent": false
    },
    "page_action":{
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",  
        "<all_urls>", 
        "storage", 
        "webNavigation", 
        "declarativeContent" ]

    
}